{
  "name": "aml-service",
  "version": "1.0.0",
  "description": "aml Service",
  "main": "src/app.ts",
  "private": true,
  "engines": {
    "node": "20.11.1",
    "npm": "10.5.0"
  },
  "scripts": {
    "build": "tsc",
    "start": "node --inspect dist/app.js",
    "prestart": "npm run build",
    "start:dev": "nodemon --watch '*.ts' --exec 'ts-node' ./src/app.ts",
    "start:dev:win": "nodemon --exec npx ts-node ./src/app.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "http-status": "^1.7.4",
    "joi": "^17.12.3",
    "lodash": "^4.17.21",
    "pg": "^8.12.0",
    "typeorm": "^0.3.20",
    "uid": "^2.0.2",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.7",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^2.1.1",
    "husky": "^8.0.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4"
  }
}
